{
	"rules": [{
		"name": "calls",
		"description": "Large duration calls",
		"notes": "Includes only those that are longer than 2 mins",
		"operators": [{
			"level": 1,
			"handler": "handler_table_apply_rule",
			"params": {
				"table": "calls",
				"rule": {
					"column": "Duration",
					"match": "GT",
					"values": 60
				},
				"metrics": [{
						"name": "total",
						"handler": "total",
						"args": {
							"feature": "%(owner)s__%(level)s__%(rule_name)s__xx__%(suffix)s"
						}
					},
					{
						"name": "dates",
						"handler": "dates",
						"args": {
							"suffix": "days"
						}
					}
				]
			}
		}]
	}]
}
